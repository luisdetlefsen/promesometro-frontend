{
    "identityPoolName": "promesometrofrontend",
    "allowUnauthenticatedIdentities": true,
    "lambdaLogPolicy": "cognitopromesometro_lambda_log_policy",
    "openIdLambdaRoleName": "cognitopromesometro_openid_lambda_role",
    "openIdRolePolicy": "cognitopromesometro_openid_pass_role_policy",
    "openIdLambdaIAMPolicy": "cognitopromesometro_openid_lambda_iam_policy",
    "openIdLogPolicy": "cognitopromesometro_openid_lambda_log_policy",
    "userPoolName": "promesometrocandidatos",
    "autoVerifiedAttributes": [
        "phone_number"
    ],
    "mfaConfiguration": "OFF",
    "mfaTypes": [
        "SMS Text Message"
    ],
    "roleName": "cognitopromesometro_sns-role",
    "roleExternalId": "cognitopromesometro_role_external_id",
    "policyName": "cognitopromesometro-sns-policy",
    "smsAuthenticationMessage": "Your authentication code is {####}",
    "smsVerificationMessage": "Your verification code is {####}",
    "emailVerificationSubject": "Your verification code",
    "emailVerificationMessage": "Your verification code is {####}",
    "defaultPasswordPolicy": true,
    "passwordPolicyMinLength": 8,
    "passwordPolicyCharacters": [
        "Requires Lowercase",
        "Requires Uppercase",
        "Requires Numbers"
    ],
    "requiredAttributes": [
        "address",
        "birthdate",
        "email",
        "family_name",
        "middle_name",
        "gender",
        "name",
        "phone_number",
        "picture"
    ],
    "userpoolClientName": "cognitopromesometro_app_client",
    "userpoolClientGenerateSecret": true,
    "userpoolClientRefreshTokenValidity": 30,
    "userpoolClientWriteAttributes": [
        "given_name",
        "nickname",
        "website",
        "address",
        "birthdate",
        "email",
        "family_name",
        "middle_name",
        "gender",
        "name",
        "phone_number",
        "picture"
    ],
    "userpoolClientReadAttributes": [
        "address",
        "birthdate",
        "email",
        "family_name",
        "middle_name",
        "gender",
        "locale",
        "given_name",
        "name",
        "nickname",
        "phone_number",
        "preferred_username",
        "picture",
        "profile",
        "updated_at",
        "website",
        "zoneinfo",
        "email_verified",
        "phone_number_verified"
    ],
    "mfaLambdaRole": "cognitopromesometro_totp_lambda_role",
    "mfaLambdaLogPolicy": "cognitopromesometro_totp_lambda_log_policy",
    "mfaPassRolePolicy": "cognitopromesometro_totp_pass_role_policy",
    "mfaLambdaIAMPolicy": "cognitopromesometro_totp_lambda_iam_policy",
    "userpoolClientLambdaRole": "cognitopromesometro_userpoolclient_lambda_role",
    "userpoolClientLogPolicy": "cognitopromesometro_userpoolclient_lambda_log_policy",
    "userpoolClientLambdaPolicy": "cognitopromesometro_userpoolclient_lambda_iam_policy",
    "userpoolClientSetAttributes": true,
    "useDefault": "manual",
    "authSelections": "identityPoolAndUserPool",
    "resourceName": "cognitopromesometro",
    "thirdPartyAuth": false,
    "hostedUI": false,
    "authRoleName": {
        "Ref": "AuthRoleName"
    },
    "unauthRoleName": {
        "Ref": "UnauthRoleName"
    },
    "authRoleArn": {
        "Fn::GetAtt": [
            "AuthRole",
            "Arn"
        ]
    },
    "unauthRoleArn": {
        "Fn::GetAtt": [
            "UnauthRole",
            "Arn"
        ]
    }
}